<Window x:Class="Modeler.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RenameWindow" Height="170" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0">Задайте новое значение имени элемента</Label>
        <TextBox Name="NewNameTextBox" HorizontalAlignment="Stretch" Grid.Row="1" Margin="5"></TextBox>
        <ComboBox Grid.Row="2" SelectedIndex="0" Name="TypeModel">
            <ComboBoxItem Content="Поток управления"></ComboBoxItem>
            <ComboBoxItem Content="Данные"></ComboBoxItem>
            <ComboBoxItem Content="Ресурсы"></ComboBoxItem>
            <ComboBoxItem Content="Исключения"></ComboBoxItem>
        </ComboBox>
        <Button Grid.Row="3" Click="ButtonBase_OnClick" HorizontalAlignment="Center" Margin="5" Focusable="True">Сохранить</Button>
    </Grid>
</Window>
